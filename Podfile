platform :ios, '15.6'

target 'Cleaner8-Xu2' do
  use_frameworks!

  # Firebase 分析
  pod 'FirebaseAnalytics'
  
  pod 'FirebaseRemoteConfig'
  
  # AppsFlyer 归因分析
  pod 'AppsFlyerFramework'

  # ThinkingData（数数）用户行为分析
  pod 'ThinkingSDK'
  
  # 启动页动画
  pod 'lottie-ios'

  target 'Cleaner8-Xu2Tests' do
    inherit! :search_paths
  end

  target 'Cleaner8-Xu2UITests' do
    inherit! :search_paths
  end
end
